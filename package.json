{
  "name": "typescript-node-express-mongodb-backend",
  "version": "5.0.1",
  "main": "index.ts",
  "author": "HADMARINE",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && pm2 start -i 0",
    "dev": "nodemon",
    "build": "babel src --out-dir dist --extensions '.ts'",
    "tsc": "tsc",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "test": "npx jest --config ./jest.config.js --watch --detectOpenHandles --forceExit --testTimeout=10000",
    "test-auto": "npx jest --config ./jest.config.js --forceExit",
    "enc:env": "openssl aes-256-cbc -e -in .env -out .env.pub -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "dec:env": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "enc:env-m": "openssl aes-256-cbc -e -in .env -out .env.pub -md md5 -k",
    "dec:env-m": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k",
    "berry": "yarn set version berry",
    "latest": "yarn set version latest",
    "classic": "yarn set version classic"
  },
  "dependencies": {
    "aws-sdk": "^2.872.0",
    "chalk": "^4.1.0",
    "clear-logger": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "deasync": "^0.1.21",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-ipfilter": "^1.1.2",
    "express-quick-builder": "^1.0.4",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.5",
    "require-strip-json-comments": "^2.0.0",
    "socket.io": "^4.0.0",
    "strip-json-comments": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/deasync": "^0.1.1",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/express-rate-limit": "^5.1.1",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/jest-expect-message": "^1.0.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.4",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/node-cron": "^2.0.3",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^24.1.5",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-lint": "^4.5.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.2.3"
  }
